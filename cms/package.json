{
  "name": "conexao-dnit",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "2.0.1",
  "private": true,
  "main": "./app",
  "author": "Gledson Assis <gledson.dev@gmail.com>",
  "license": "MIT",
  "locale": "pt-BR",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "type-check": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "tslint": "tslint --fix -p . -c tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "run lint && npm run test:run",
    "format:eslint": "prettier-eslint --eslint-config-path ./.eslintrc.js --write '**/*.js'",
    "heroku-postbuild": "npm run build",
    "build:staging": "env-cmd -f .env.$APP_ENV next build"
  },
  "sideEffects": [
    "./next-i18next.config.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "prettier --write",
      "git add",
      "jest --findRelatedTests",
      "tslint"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "dependencies": {
    "axios": "0.21.4",
    "cookie-cutter": "0.2.0",
    "debounce": "1.2.1",
    "env-cmd": "10.1.0",
    "eslint-cli": "1.1.1",
    "flatpickr": "4.6.9",
    "inputmask": "5.0.6",
    "moment": "2.29.1",
    "next": "12.0.1",
    "next-i18next": "8.8.0",
    "next-images": "1.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-draft-wysiwyg": "1.14.7",
    "react-hook-form": "7.18.0-next.0",
    "react-input-mask": "3.0.0-alpha.2",
    "react-notifications": "1.7.2",
    "react-number-format": "4.7.3",
    "react-rating": "2.0.5",
    "react-redux": "7.2.5",
    "react-router-dom": "5.3.0",
    "react-share": "4.4.0",
    "react-summernote": "2.0.2",
    "react-toastify": "8.1.0",
    "react-trix": "0.9.0",
    "redux-saga": "1.1.3",
    "route-parser": "0.0.5",
    "sass": "1.41.1",
    "suneditor": "2.41.3",
    "suneditor-react": "3.3.0",
    "ts-jest": "27.0.5",
    "typed-redux-saga": "1.3.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.0",
    "@types/enzyme": "3.10.9",
    "@types/inputmask": "5.0.1",
    "@types/jest": "27.0.1",
    "@types/node": "16.9.4",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/react-input-mask": "3.0.1",
    "@types/react-router-dom": "5.1.9",
    "@types/route-parser": "0.1.3",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
    "@zeit/next-typescript": "1.1.1",
    "babel-jest": "27.2.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.3",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.2.0",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "prettier-eslint": "13.0.0",
    "prettier-eslint-cli": "5.0.1",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.4.3",
    "webpack": "5.53.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/cms/src$1",
      "@/tests/(.*)": "<rootDir>/cms/tests/$1",
      "@/(.*)": "<rootDir>/cms/src$1"
    }
  }
}
